<%@ include file="/include/head.jsp"%>
	<link rel="stylesheet" type="text/css" href="/css/systemerrorinfo.css"/>
	<script type="text/javascript" src="/js/systemerrorinfo.js"></script>
	<table cellpadding ="0" >
		<colgroup>
			<col class="col-title">
			<col class="col-detail">
		</colgroup>
		<thead>
			<tr>
				<td colspan="2" class="table-head"> System Error </td>
			</tr>
		</thead>
		<tbody>
			<tr>
				<td>From:</td>
				<td>${error.from }</td>
			</tr>
			<tr>
				<td>Status:</td>
				<td>${error.status }</td>
			</tr>
			<tr>
				<td>CreateTime:</td>
				<td>${error.createTime}</td>
			</tr>
			<tr>
				<td>UpdateTime:</td>
				<td>${error.updateTime }</td>
			</tr>
			<tr>
				<td>ExternInfo:</td>
				<td>${error.externInfo }</td>
			</tr>
			<tr>
				<td>Info:</td>
				<td>${error.info}</td>
			</tr>
		</tbody>
	</table>
<%@ include file="/include/foot.jsp"%>